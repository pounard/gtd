{
    "name": "pounard/gtd",
    "license": "proprietary",
    "type": "project",
        "repositories" : [{
            "type" : "vcs",
            "url" : "git@github.com:makinacorpus/php-acl.git"
        }, {
            "type" : "vcs",
            "url" : "git@github.com:pounard/goat.git"
        }, {
            "type" : "vcs",
            "url" : "git@github.com:pounard/goat-bundle.git"
        }, {
            "type" : "vcs",
            "url" : "git@github.com:pounard/goat-bundle-account.git"
        }, {
            "type" : "vcs",
            "url" : "git@github.com:makinacorpus/redis-bundle.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "": "src/",
            "Goat\\Bundle\\" : "vendor/makinacorpus/goat-bundle/",
            "Goat\\AccountBundle\\" : "vendor/makinacorpus/goat-bundle-account/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable" : true,
    "require": {
        "friendsofsymfony/rest-bundle": "^2.1",
        "incenteev/composer-parameter-handler": "^2.0",
        "jenssegers/optimus": "^0.2.1",
        "jms/serializer-bundle": "^1.1",
        "makinacorpus/php-acl": "dev-master",
        "makinacorpus/goat": "dev-master",
        "makinacorpus/goat-bundle": "dev-master",
        "makinacorpus/goat-bundle-account": "dev-master",
        "makinacorpus/redis-bundle": "dev-master",
        "php": ">=5.5.9",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "symfony/monolog-bundle": "^2.8",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/psr-http-message-bridge": "^1.0",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/symfony": "3.3.*",
        "erusev/parsedown": "^1.6",
        "twig/extensions": "^1.4",
        "sabre/dav": "^3.2",
        "dompdf/dompdf": "^0.8.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
