{
    "name": "pounard/gtd",
    "description": "Getting Things Done",
    "license": "proprietary",
    "type": "project",
    "minimum-stability" : "dev",
    "prefer-stable" : true,
    "require": {
        "erusev/parsedown": "^1.6",
        "makinacorpus/calista-core": "^3.0.0-alpha12",
        "makinacorpus/goat": "^4.0.0-alpha6",
        "makinacorpus/goat-query": "^3.0.0-alpha3",
        "makinacorpus/goat-query-bundle": "^3.0.0",
        "php": "^8.0",
        "ramsey/uuid": "^4",
        "sabre/dav": "^3.2",
        "symfony/asset": "5.2.*",
        "symfony/browser-kit": "5.2.*",
        "symfony/cache": "5.2.*",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "*",
        "symfony/expression-language": "5.2.*",
        "symfony/flex": "^1.1",
        "symfony/form": "5.2.*",
        "symfony/framework-bundle": "5.2.*",
        "symfony/http-client": "5.2.*",
        "symfony/mime": "^5.2",
        "symfony/monolog-bundle": "^3.5",
        "symfony/polyfill-apcu": "^1.11",
        "symfony/process": "5.2.*",
        "symfony/property-info": "5.2.*",
        "symfony/security-bundle": "5.2.*",
        "symfony/serializer-pack": "^1.0",
        "symfony/twig-bundle": "5.2.*",
        "symfony/validator": "5.2.*",
        "symfony/webpack-encore-bundle": "^1.0",
        "symfony/yaml": "5.2.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "jdorn/sql-formatter": "^1.2",
        "symfony/phpunit-bridge": "5.1.*",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "platform": {
            "php": "8.0.1"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "src/Application/Location/Command.php",
            "src/Application/Location/Query.php",
            "src/Domain/Location/Event.php"
        ],
        "psr-4": {
            "Gtd\\Application\\": "src/Application",
            "Gtd\\Domain\\": "src/Domain",
            "Gtd\\Generated\\": "generated/",
            "Gtd\\Infrastructure\\": "src/Infrastructure",
            "Gtd\\Persistence\\": "src/Persistence",
            "Gtd\\Shared\\": "src/Shared",
            "Gtd\\Symfony\\": "src/Symfony",
            "MakinaCorpus\\CoreBus\\": "packages/corebus/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
             "Gtd\\Tests\\": "tests/"
        }
    },
    "replace": {
        "makinacorpus/corebus": "1.*",
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    }
}
